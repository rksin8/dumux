[Grid]
UpperRight = 1 1
Cells = 5 5

[Problem]
Name = test_donea # name passed to the output routines
EnableGravity = false
PrintL2Error = false
EnableInertiaTerms = false

[Component]
LiquidDensity  = 1.0
LiquidKinematicViscosity = 1.0

[ Newton ]
MaxRelativeShift = 1e-5
EnableShiftCriterion = false
EnableSIMPLEAbsoluteResidualCriterion = true
MaxSteps = 500
TargetSteps = 500
MaxTimeStepDivisions = 0

[Vtk]
WriteFaceData = false

[ InitialCondition ]
# 0: zero
# 1: exact solution
InitialConditionType = 1

[Algorithm]
AlgorithmType = 2
